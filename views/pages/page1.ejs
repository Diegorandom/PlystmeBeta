                                <td><%=instrumental2.toFixed(3)%>%</td>
                            </tr>
                            <tr>
                                <th>Liveness</th>
                                <td><%=audiencia.toFixed(3)%>%</td>
                                <td><%=audiencia2.toFixed(3)%>%</td>
                            </tr>
                            <tr>
                                <th>Valence</th>
                                <td><%=positivismo.toFixed(3)%>%</td>
                                <td><%=positivismo2.toFixed(3)%>%</td>
                            </tr>
                            <tr>
                                <th>Tempo</th>
                                <td><%=tempo.toFixed(3)%> BPM</td>
                                <td>Not processed data</td>
                            </tr>
                            <tr>
                                <th>Duration</th>
                                <td><%=duracion%> mS</td>
                                <td>not processed data</td>
                            </tr>
                            <tr>
                                <th>Beats per bar</th>
                                <td><%=Math.round(firma_tiempo)%></td>
                                <td>Not processed data</td>
                            </tr>
                        </tbody>
                    </table>
                    
                    
                    
					
